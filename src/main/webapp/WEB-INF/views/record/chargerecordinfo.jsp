<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%>
<%@ taglib prefix="fmt" uri="http://java.sun.com/jsp/jstl/fmt"%>
<!DOCTYPE html>
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
<title>充电详情</title>
<%@ include file="/WEB-INF/views/public/commons.jspf"%>
<%@ include file="/WEB-INF/views/public/compWechatFont.jspf"%>
<link rel="stylesheet" href="${hdpath}/css/base.css"/>
<style>
	body {
		background-color: #fff;
		display: inline-block;
	} 
	.app {
		color: #666;
		font-size: 14px;
	}
	.money_img {
		width: 20vw;
		display: block;
		margin: 10vh auto 15px;
	}
	.charge_top {
		text-align: center;
		line-height: 2em;
		padding-bottom: 8px;
		position: relative;
		font-size: 16px;
	}
	.charge_top:after {
		content: '';
		position: absolute;
		left: 10vw;
		height: 1px;
		right: 10vw;
		bottom: 0;
		background-color: #dfdfdf;
	}
	.charge_info{
		padding-top: 8px;
		width: 80vw;
		margin: 0 auto;
	}
	.charge_info li div{
		padding: 4px 0;
		display: flex;
		justify-content: space-between;
		flex-wrap: wrap;
	}
	.charge_info li div span:first-child {
		color: #333;
		flex-shrink: 0;
	}
	.charge_info li div span:last-child {
		color: #777;
	}
	.charge_info li div span.red{
		color: #F3686D;
	}
	.charge_info li div span.origin{
		color: #FF7F27;
	}
	.charge_info li div span.green{
		color: #22B14C;
	}
	@media screen and (max-width: 319px) {
		.charge_info{
			width: 90vw;
		}
		.charge_top:after {
			left: 5vw;
			right: 5vw;
		}
	}
</style>
</head>
<body>
<div class="app">
	<img class="money_img" src="data:image/jpg;base64," alt="">
	<div class="charge_top">
		充电金额： <fmt:formatNumber type="number" value="${useMoney - refundMoney}" maxFractionDigits="2" pattern="0.00"/>元
	</div>
	<ul class="charge_info">
		<li>
			<div><span>订单编号：</span><span>${chargeinfo.ordernum}</span></div>
			<div><span>支付方式：</span>
				<span>
					<c:choose>
						<c:when test="${chargeinfo.paytype == 1}"><font>钱包付款</font></c:when>
						<c:when test="${chargeinfo.paytype == 2}"><font>微信付款</font></c:when>
						<c:when test="${chargeinfo.paytype == 3}"><font>支付宝付款</font></c:when>
					</c:choose>
				</span>
			</div>
			<div><span>设备信息：</span><span>${chargeinfo.equipmentnum}-${chargeinfo.port}</span></div>
			<div><span>设备名称：</span><span>${deviceInfo.remark == null ? '— —' : deviceInfo.remark}</span></div>
			<div><span>小区名称：</span><span>${deviceInfo.areaname == null ? '— —' : deviceInfo.areaname}</span></div>
			<div><span>充电时长：</span><span>${chargeinfo.consumeTime}分钟</span></div>
			<div><span>开始时间：</span><span><fmt:formatDate value="${chargeinfo.begintime}" pattern="yyyy-MM-dd HH:mm:ss" /></span></div>
			<c:if test="${chargeinfo.number == 0}">
				<div><span>记录状态：</span><span class="green">正常</span></div>
			</c:if>
			<c:if test="${chargeinfo.number == 1}">
				<div><span>记录状态：</span><span class="red">全额退款</span></div>
			</c:if>
			<c:if test="${chargeinfo.number == 2}">
				<div><span>记录状态：</span><span class="origin">部分退款<b>(${chargeinfo.refundMoney}元)</b></span></div>			
			</c:if>
		</li>
	</ul>
</div>
</body>
</html>